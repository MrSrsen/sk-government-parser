{
    "name": "srsen/sk-government-parser",
    "description": "Package for requesting structured data about business subjects from pages of slovak government without API.",
    "type": "library",
    "version": "2.0.0",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "ext-dom": "20031129",
        "guzzlehttp/guzzle": "^7.0"
    },
    "support": {
        "source": "https://github.com/MrSrsen/sk-government-parser",
        "issues": "https://github.com/MrSrsen/sk-government-parser/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Sršeň",
            "email": "martin@srsen.sk"
        }
    ],
    "autoload": {
        "psr-4":{
            "SkGovernmentParser\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
